{
  "name": "react-scss-boilerplate",
  "version": "2.0.0",
  "description": "Minimalistic ES6 React boilerplat with SCSS support",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run production",
    "start": "webpack-dev-server",
    "clean": "rimraf public/assets/",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint app",
    "test": "mocha --compilers js:babel-core/register --recursive ./tests/**/*.js",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "MD Sultan Nasir Uddin<manikdrmc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^3.2.3",
    "history": "^2.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^2.5.2",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-boilerplate": "^1.1.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.27.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.0.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
